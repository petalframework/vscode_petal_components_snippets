{
    "name": "vscode-petal-components-snippets",
    "displayName": "Petal Components (HEEX) snippets",
    "description": "Code snippets for the Petal Components (HEEX) library",
    "publisher": "petalframework",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Snippets"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "icon": "images/icon.png",
    "contributes": {
        "snippets": [
            {
                "language": "phoenix-heex",
                "path": "./snippets/heex.code-snippets"
            }
        ]
    },
    "keywords": [
        "snippets",
        "phoenix",
        "elixir"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/petalframework/vscode_petal_components_snippets"
    }
}
